body
html
  color #212121  
  font-size 1em
h1
  font-size 2em
h4
  font-size 1.1em

.hide
  display none

/*long header style*/
.bottom-header
  height 1%

.long-header
  height 30%
  width 100%
  text-align center

/*bar style*/
.title
.bar-header .button
ion-header-bar i
  color #fff

ion-header-bar
  height 56px
  color #fff

.bar-header
  border 0

.has-long-header
  top 31% !important

/*side menu*/
ion-side-menu ion-content
ion-side-menu ion-content .item-complex a.item-content
  background-color #C6C6C6

/* Palette generated by Material Palette - materialpalette.com/brown/cyan */

.dark-primary-color    
  background: #0097A7
.default-primary-color 
  background: #00BCD4
.light-primary-color   
  background: #B2EBF2
.text-primary-color    
  color: #FFFFFF
.accent-color          
  color: #FF9800
.primary-text-color    
  color: #212121
.secondary-text-color  
  color: #727272
.divider-color         
  border-color: #B6B6B6
.footer-color
  background: #F4F4F4
.accent-primary-color
  background: #FF9800
